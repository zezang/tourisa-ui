{
    "compilerOptions": {
        "target": "es6",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext",
        ],
        "baseUrl": "./src",
        "forceConsistentCasingInFileNames": true,
        "jsx": "react-jsx",
        "module": "esnext",
        "moduleResolution": "node",
        "noEmit": true,
        "noFallthroughCasesInSwitch": true,
        "resolveJsonModule": true,
        "strict": true,
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}